      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="weight" type="smallint" column="weight" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="bmi" type="smallint" column="bmi" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sleeping" type="integer" column="sleeping" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="awake" type="integer" column="awake" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="awakening" type="integer" column="awakening" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="inBed" type="integer" column="in_bed" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="calories" type="integer" column="calories" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="steps" type="integer" column="steps" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="distance" type="integer" column="distance" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="floors" type="smallint" column="floors" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sedentary" type="integer" column="sedentary" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mobile" type="integer" column="mobile" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="active" type="integer" column="active" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="veryActive" type="integer" column="very_active" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
